<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="xuat_chieu_action" model="ir.actions.act_window">
        <field name="name">Xuất chiếu</field>
        <field name="res_model">cinema.xuat.chieu</field>
        <field name="view_mode">tree,form</field>
    </record>


    <!--List cac show time voi cac thuoc tinh hien thi them o giao dien-->
    <record id="xuat_chieu_view_tree" model="ir.ui.view">
        <field name="name">cinema.xuat.chieu.tree</field>
        <field name="model">cinema.xuat.chieu</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <field name="name"/>
                <field name="room_id"/>
                <field name="start_time"/>
            </tree>
        </field>
    </record>

</odoo>