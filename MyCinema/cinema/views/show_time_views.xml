<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="show_time_action" model="ir.actions.act_window">
        <field name="name">Xuất chiếu</field>
        <field name="res_model">show.time</field>
        <field name="view_mode">tree,form</field>
    </record>


    <!--List cac show time voi cac thuoc tinh hien thi them o giao dien-->
    <record id="show_time_view_tree" model="ir.ui.view">
        <field name="name">show.time.tree</field>
        <field name="model">show.time</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <field name="begin_time"/>
                <field name="end_time"/>
                <field name="movie_ids">
                    <tree>
                        <field name="name"/>
                        <field name="length"/>
                    </tree>
                </field>
            </tree>
        </field>
    </record>

</odoo>